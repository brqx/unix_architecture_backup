#+ Alias definidos utiles para procesos basados en scripts
#-------------------------------------------------------------------
alias pjars="${C_RCT}scripts/procesos/testjars.x"
#-------------------------------------------------------------------

#- Alias de chequeo de jars para comprobar si estan corruptos
#-------------------------------------------------------------------
alias testjars='for i in $( f jar ) ; do jar -tf $i &> /dev/null; if [ "$?" = "0" ] ; then  echo "$i OK"; else echo "$i ERROR"; fi; done '
#-------------------------------------------------------------------
alias testjerror='testjars | grep ERROR'


