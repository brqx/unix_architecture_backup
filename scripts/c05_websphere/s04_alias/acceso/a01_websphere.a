#-- Alias de acceso a proyectos
#-====================================-

#-------------------------------------------------------------------
#- Websphere Application Server - Generic & Cell - Cluster
#-------------------------------------------------------------------

#- Ir al home de Websphere
alias gowas='cd ${WAS_HOME}bin/'

#- Ir al home de Nodo Principal - Perfil por defecto
alias gonodo='cd ${WPS_PD}bin/'

#- Ir al home del Dmgr - Manager o Perfil por defecto en su ausencia
alias godmgr='cd ${WPS_DM}bin/'


#-------------------------------------------------------------------
#- Process Server
#-------------------------------------------------------------------

#- Ir al home de Websphere
alias gowps='cd ${WPS_HOME}'

#- Acceso al BIN
alias gr='cd ${WPS_RUN}'

#- Gestion de Perfiles existentes
alias gopf='cd ${WPS_PF}'

#- Generacion de perfiles
alias gopm='cd ${WPS_PM}'

#-------------------------------------------------------------------
#- Business Monitor
#-------------------------------------------------------------------

#- Ir al home del Monitor
alias gowbm='cd ${WBM_HOME}'

#- Acceso al BIN
alias gowbmrun='cd ${WBM_RUN}'

#- Gestion de Perfiles existentes
alias gowbmpf='cd ${WBM_PF}'

#- Generacion de perfiles
alias gowbmpm='cd ${WBM_PM}'


