#- Ir al home de Websphere FRONTAL - WAS
alias logfron='cd ${LOGS_FRONT}'

#- Ir a los logs de aplicaciones desplegadas tradicionalmente
alias logapps='cd ${WPS_LOGS_APLICACIONES}'

#- Ir a los logs de aplicaciones desplegadas tradicionalmente
alias logarq='cd ${WPS_LOGS_APLICACIONES}'

#- Ir a los logs de aplicaciones desplegadas via maven - integracion continua
alias logmav='cd ${WPS_LOGS_MAVEN}'

#- Alias clear logs

alias deleteserverlogs='rm -f /logs/*/*/*/*/*/*/*'
